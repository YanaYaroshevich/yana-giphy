{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": [
		"react",
		"prettier",
		"jest"
	],
	"env": {
		"jest/globals": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"rules": {
		"react/prop-types": 0,
		"prettier/prettier": "error"
	}
}
